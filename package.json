{
  "name": "framework7-source",
  "version": "0.0.0",
  "scripts": {
    "/*============ Build ============*/": "============",
    "build:dev": "cross-env-shell NODE_ENV=development \"gulp build-core && gulp build-vue && gulp build-react && gulp build-svelte\"",
    "build:prod": "cross-env-shell NODE_ENV=production \"gulp build-core && gulp build-vue && gulp build-react && gulp build-svelte\"",
    "build-core-icons-font": "python ./scripts/icon-font/generate.py && ttf2woff ./src/core/icons/font/framework7-core-icons.ttf ./src/core/icons/font/framework7-core-icons.woff",
    "build-core:dev": "cross-env NODE_ENV=development CORE_BUILD_ONLY_UMD=true gulp build-core",
    "build-core:prod": "cross-env NODE_ENV=production gulp build-core",
    "build-vue:dev": "cross-env-shell NODE_ENV=development \"gulp build-vue\"",
    "build-vue:prod": "cross-env-shell NODE_ENV=production \"gulp build-vue\"",
    "build-react:dev": "cross-env-shell NODE_ENV=development CORE_BUILD_ONLY_MODULES=true \"gulp build-react\"",
    "build-react:prod": "cross-env-shell NODE_ENV=production \"gulp build-react\"",
    "build-svelte:dev": "cross-env-shell NODE_ENV=development \"gulp build-svelte\"",
    "build-svelte:prod": "cross-env-shell NODE_ENV=production \"gulp build-svelte\"",
    "ks:dev": "cross-env-shell NODE_ENV=development \"gulp ks-core && gulp ks-react && gulp ks-vue && gulp ks-svelte\"",
    "ks:prod": "cross-env-shell NODE_ENV=production \"gulp ks-core && gulp ks-react && gulp ks-vue && gulp ks-svelte\"",
    "/*============ KS Core ============*/": "============",
    "core": "cross-env-shell NODE_ENV=development CORE_BUILD_ONLY_UMD=true \"gulp build-core && gulp ks-core && gulp server-core\"",
    "/*============ KS React ============*/": "============",
    "react:dev": "cross-env-shell NODE_ENV=development CORE_BUILD_ONLY_MODULES=true  \"gulp build-core && gulp build-react\"",
    "react:watch": "cross-env-shell NODE_ENV=development CORE_BUILD_ONLY_MODULES=true  \"gulp watch-react\"",
    "react:webpack": "cross-env NODE_ENV=development webpack-dev-server --config ./scripts/webpack-react.config.js",
    "react": "npm run react:dev && concurrently --kill-others \"npm run react:webpack\" \"npm run react:watch\"",
    "/*============ KS Vue ============*/": "============",
    "vue:dev": "cross-env-shell NODE_ENV=development CORE_BUILD_ONLY_MODULES=true  \"gulp build-core && gulp build-vue\"",
    "vue:watch": "cross-env-shell NODE_ENV=development CORE_BUILD_ONLY_MODULES=true  \"gulp watch-vue\"",
    "vue:webpack": "cross-env NODE_ENV=development webpack-dev-server --config ./scripts/webpack-vue.config.js",
    "vue": "npm run vue:dev && concurrently --kill-others \"npm run vue:webpack\" \"npm run vue:watch\"",
    "/*============ KS Svelte ============*/": "============",
    "svelte:dev": "cross-env-shell NODE_ENV=development CORE_BUILD_ONLY_MODULES=true  \"gulp build-core && gulp build-svelte\"",
    "svelte:watch": "cross-env-shell NODE_ENV=development CORE_BUILD_ONLY_MODULES=true  \"gulp watch-svelte\"",
    "svelte:webpack": "cross-env NODE_ENV=development webpack-dev-server --config ./scripts/webpack-svelte.config.js",
    "svelte": "npm run svelte:dev && concurrently --kill-others \"npm run svelte:webpack\" \"npm run svelte:watch\"",
    "/*============ Dev tools ============*/": "============",
    "prettier": "prettier \"**/*.+(js|json|scss|css|less|ts|vue)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "lint": "eslint --ext .js,.vue,.jsx .",
    "validate": "npm-run-all --parallel check-format lint",
    "test": "npm run build:dev && npm run lint",
    "release": "npm run validate && node ./scripts/release",
    "postinstall": "echo \"\u001b[35m\u001b[1mLove Framework7? Support project by donating or pledging on patreon:\u001b[22m\u001b[39m\n > \u001b[32mhttps://patreon.com/vladimirkharlampidi\u001b[0m\n\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/framework7io/framework7.git"
  },
  "author": "Vladimir Kharlampidi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/framework7io/framework7/issues"
  },
  "homepage": "http://framework7.io/",
  "funding": {
    "type": "patreon",
    "url": "https://www.patreon.com/vladimirkharlampidi"
  },
  "browserslist": [
    "Android >= 7",
    "IOS >= 11",
    "Safari >= 11",
    "Chrome >= 49",
    "Firefox >= 52",
    "Samsung >= 6"
  ],
  "dependencies": {
    "dom7": "^3.0.0-alpha.9",
    "path-to-regexp": "^6.1.0",
    "skeleton-elements": "^3.0.0",
    "ssr-window": "^3.0.0-alpha.4",
    "swiper": "^6.3.0",
    "template7": "^1.4.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.4",
    "@babel/plugin-transform-react-jsx": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@rollup/plugin-babel": "^5.2.0",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@vue/compiler-sfc": "^3.0.0",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.1.0",
    "clean-css": "^4.2.3",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "css-loader": "^4.3.0",
    "eslint": "^7.7.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-svelte3": "^2.7.3",
    "eslint-plugin-vue": "^7.0.0-beta.3",
    "exec-sh": "^0.3.4",
    "glob": "^7.1.6",
    "gulp": "^4.0.2",
    "gulp-connect": "^5.7.0",
    "html-webpack-plugin": "^4.4.1",
    "husky": "^4.2.5",
    "inquirer": "^7.3.3",
    "less": "^3.12.2",
    "mini-css-extract-plugin": "^0.11.1",
    "npm-run-all": "^4.1.5",
    "opn": "^6.0.0",
    "postcss": "^7.0.32",
    "prettier": "^2.1.1",
    "prettier-plugin-svelte": "^1.4.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rollup": "^2.26.11",
    "style-loader": "^1.2.1",
    "svelte": "^3.28.0",
    "svelte-loader": "^2.13.6",
    "terser": "^5.3.0",
    "ttf2woff": "^2.0.2",
    "vue": "^3.0.0",
    "vue-loader": "^16.0.0-beta.7",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
